#include <iostream>
using namespace std;

int main()
{
    cout << " \t To be \n";
    cout << "\n ";
    cout << "\t \t or not \n";
    cout << "\n ";
    cout << "\t \t \tto be... \n";
    cout << "\n ";
    cout << "\t \t \t \t\\ William Shakespeare \\ \n";
}

int main() 
{
	cout << "\t \t   March 2025 \n";
	cout << "\t Mon Tue Wed Thu Fri Sat Sun \n";
	cout << "\t                      1   2 ";
	cout << "\n";
	cout << "\t  3   4   5   6   7   8   9 ";
	cout << "\n";
	cout << "\t 10  11  12  13  14  15  16 ";
	cout << "\n";
	cout << "\t 17  18  19  20  21  22  23 ";
	cout << "\n";
	cout << "\t 24  25  26  27  28  29  30  ";
	cout << "\n";
	cout << "\t 31 ";
}
